target 'DCDicomIOS' do
    platform :ios, '8.0'
    podspec :path => 'DCDicom.podspec'

    target 'DCDicomIOSTests' do
      inherit! :search_paths

    end
end

target 'DCDicomMacOS' do
    platform :osx, '10.9'
    podspec :path => 'DCDicom.podspec'

    target 'DCDicomMacOSTests' do
      inherit! :search_paths

    end

end
